{
  "name": "hapi-middleman-paypal-intacct",
  "version": "0.0.31",
  "description": "hapi-middleman module for integrating paypal with intacct",
  "license": "MIT",
  "repository": "",
  "author": "Andrew Throener",
  "keywords": [
    "Hapi",
    "plugin",
    "paypal",
    "PayPal",
    "Intacct",
    "integrate"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib coverage",
    "lint": "tslint --project tsconfig.json --force --format verbose \"src/**/*.ts\"",
    "build": "yarn run clean && yarn run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "nyc tape test/**/*-spec.ts dotenv_config_path=./test/.env | tap-spec; nyc report ---reporter=text",
    "watch": "yarn run build -- --watch",
    "watch:test": "yarn run test -- --watch",
    "release": "yarn test && yarn run build && yarn version --new-version --",
    "release:quick": "yarn test && yarn run build && git add -A && git commit -m 'Quick Release Patch' && yarn version --new-version patch",
    "reinstall": "rimraf node_modules yarn.lock && yarn install",
    "start": "ts-node -r dotenv/config ./example/server.ts"
  },
  "dependencies": {
    "hapi-intacct": "^0.0.20",
    "hapi-paypal": "^0.0.67",
    "later": "^1.2.0"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/dotenv": "^4.0.0",
    "@types/hapi": "^16.1.6",
    "@types/later": "^1.2.3",
    "@types/node": "^7.0.0",
    "@types/sinon": "^2.3.3",
    "blue-tape": "^1.0.0",
    "coveralls": "^2.0.0",
    "dotenv": "^4.0.0",
    "glue": "^4.2.0",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.4.3",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "sinon": "^3.2.1",
    "tap-spec": "^4.1.1",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "dotenv/config"
    ],
    "reporter": []
  }
}
